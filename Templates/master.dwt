<body class="drawer drawer-left">
<header role="banner">
	<div class="head-bar">
		<h1 class="page-title">患者情報</h1>
		<ul class="utility-nav">
			<li class="is-search-btn"><a href="#"><i class="iconfont-search"></i></a></li>
			<li><a href="#"><i class="iconfont-check"></i><span class="badge">24</span></a></li>
			<li><a href="#"><i class="iconfont-followers"></i><span class="badge">5</span></a></li>
		</ul>
	</div>
	<!-- / .head-bar -->
	<div class="search-panel">
		<form>
			<div class="container-fluid">
				<div class="row mb10">
					<div class="form-group col-xs-12 col-sm-6">
						<label for="patient-name" class="search-panel-label">患者名</label>
						<input type="text" class="form-control" id="patient-name" placeholder="山田　太郎">
					</div>
					<div class="form-group col-xs-12 col-sm-6">
						<label for="patient-case" class="search-panel-label">症例</label>
						<input type="text" class="form-control" id="patient-case" placeholder="症例を入力してください">
					</div>
					<div class="form-group col-xs-12 col-sm-6">
						<label for="patient-prescription" class="search-panel-label">処方</label>
						<input type="text" class="form-control" id="patient-prescription" placeholder="処方を入力してください">
					</div>
					<div class="form-group col-xs-12 col-sm-6">
						<label for="patient-deal" class="search-panel-label">処置</label>
						<input type="text" class="form-control" id="patient-deal" placeholder="処置を入力してください">
					</div>
				</div>
				<!-- / .row -->
				<div class="row mb15">
					<div class="search-panel-label col-xs-3 col-sm-1 pt05">回数</div>
					<div class="col-xs-9 col-sm-11">
						<div class="btn-toolbar" role="toolbar">
							<div class="btn-group" role="group">
								<input type="text" class="btn btn-input btn-w6em" placeholder="下限なし">
								<button type="button" class="btn btn-updown">−</button>
								<button type="button" class="btn btn-updown">＋</button>
							</div>
							<div class="btn-group btn-clear"><span class="btn btn-tsunagi">〜</span></div>
							<div class="btn-group btn-clear" role="group">
								<input type="text" class="btn btn-input btn-w6em" placeholder="上限なし">
								<button type="button" class="btn btn-updown">−</button>
								<button type="button" class="btn btn-updown">＋</button>
							</div>
						</div>
						<!-- / .btn-toolbar --> 
					</div>
				</div>
				<!-- / .row -->
				<div class="row mb15">
					<div class="search-panel-label col-xs-3 col-sm-1 pt05">受診日</div>
					<div class="col-xs-9 col-sm-11">
						<div class="btn-toolbar">
							<div class="btn-group">
								<input type="text" class="btn btn-input btn-w12em" placeholder="2013/2/15（開業日）">
							</div>
							<div class="btn-group"><span class="btn btn-tsunagi">〜</span></div>
							<div class="btn-group">
								<input type="text" class="btn btn-input btn-w12em" placeholder="本日">
							</div>
						</div>
						<!-- / .btn-toolbar --> 
					</div>
				</div>
				<!-- / .row -->
				<div class="row mb15">
					<div class="search-panel-label col-xs-3 col-sm-1">最終<br>
						受診日</div>
					<div class="col-xs-9 col-sm-11">
						<div class="btn-toolbar">
							<div class="btn-group">
								<input type="text" class="btn btn-input btn-w12em" placeholder="以上">
							</div>
							<div class="btn-group"><span class="btn btn-tsunagi">〜</span></div>
							<div class="btn-group">
								<input type="text" class="btn btn-input btn-w12em" placeholder="未満">
							</div>
						</div>
						<!-- / .btn-toolbar --> 
					</div>
				</div>
				<!-- / .row -->
				<div class="row mb15">
					<div class="search-panel-label col-xs-3 col-sm-1 pt10">年齢</div>
					<div class="col-xs-9 col-sm-11">
						<div class="btn-group" data-toggle="buttons">
							<label class="btn btn-checkbtn">
								<input type="checkbox" name="search-panel-old">
								10代以下</label>
							<label class="btn btn-checkbtn active">
								<input type="checkbox" name="search-panel-old" checked>
								10代</label>
							<label class="btn btn-checkbtn active">
								<input type="checkbox" name="search-panel-old" checked>
								20代</label>
							<label class="btn btn-checkbtn">
								<input type="checkbox" name="search-panel-old">
								30代</label>
							<label class="btn btn-checkbtn">
								<input type="checkbox" name="search-panel-old">
								40代</label>
							<label class="btn btn-checkbtn">
								<input type="checkbox" name="search-panel-old">
								50代</label>
							<label class="btn btn-checkbtn">
								<input type="checkbox" name="search-panel-old">
								60代</label>
							<label class="btn btn-checkbtn">
								<input type="checkbox" name="search-panel-old">
								70代以上</label>
						</div>
					</div>
				</div>
				<!-- / .row -->
				<div class="row mb30">
					<div class="search-panel-label col-xs-3 col-sm-1 pt10">性別</div>
					<div class="col-xs-9 col-sm-11">
						<div class="btn-group" data-toggle="buttons">
							<label class="btn btn-checkbtn">
								<input type="checkbox" name="search-panel-gender">
								男性</label>
							<label class="btn btn-checkbtn active">
								<input type="checkbox" name="search-panel-gender" checked>
								女性</label>
						</div>
					</div>
				</div>
				<!-- / .row -->
				<p class="text-center mb40">
					<button type="submit" class="btn btn-submit">この条件で検索する</button>
				</p>
			</div>
			<!-- / .container-fluid -->
		</form>
		<p class="search-panel-up"><i class="iconfont-upstop"></i></p>
	</div>
	<!-- / .search-panel -->
	<div class="container-fluid">
		<div class="drawer-header">
			<button type="button" class="drawer-toggle drawer-hamburger"><span class="sr-only">toggle navigation</span><span class="drawer-hamburger-icon"></span></button>
		</div>
		<div class="drawer-main gnav">
			<nav class="drawer-nav" role="navigation">
				<ul class="drawer-menu">
					<li class="drawer-menu-item"><a href="#"><i class="iconfont-clip"></i>トップニュース</a></li>
					<li class="drawer-menu-item"><a href="../htdocs/view-patient-info.html"><i class="iconfont-hart"></i>本日の患者</a></li>
					<li class="drawer-menu-item"><a href="#"><i class="iconfont-followers"></i>患者検索</a></li>
					<li class="drawer-menu-item"><a href="#"><i class="iconfont-summary"></i>最新サマリ</a></li>
					<li class="drawer-menu-item"><a href="#"><i class="iconfont-result"></i>検索結果</a></li>
					<li class="drawer-menu-item"><a href="#"><i class="iconfont-history"></i>患者ヒストリー</a></li>
					<li class="drawer-menu-item"><a href="#"><i class="iconfont-card"></i>過去カルテ</a></li>
					<li class="drawer-menu-item"><a href="#"><i class="iconfont-info"></i>薬剤情報</a></li>
					<li class="drawer-menu-item"><a href="#"><i class="iconfont-adjust"></i>設定</a></li>
				</ul>
				<div class="drawer-footer"><span></span></div>
			</nav>
		</div>
		<!-- / .drawer-main drawer-default --> 
	</div>
</header>
